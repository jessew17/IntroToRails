<h1>Stat#index</h1>
<p>Find me in app/views/stat/index.html.erb</p>
<table>
  <th>
    <td>Species</td>
    <td>HP</td>
    <td>Attack</td>
    <td>Armor</td>
  </th>
  <% @allStats.each do |s| %>
    <tr>
      <td><%= s.species %></td>
      <td><%= s.hp %></td>
      <td><%= s.attack %></td>
      <td><%= s.armor %></td>
    </tr>
  <% end %>
</table>